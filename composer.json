{
  "name": "iwannamaybe/phpcas",
  "description": "PhpCas client for the Laravel Framework",
  "keywords": [
    "iwannamaybe",
    "laravel",
    "phpcas"
  ],
  "license": "MIT",
  "homepage": "https://www.iwannamaybe.com",
  "support": {
    "issues": "https://github.com/iwannamaybe/phpcas/issues",
    "source": "https://github.com/iwannamaybe/phpcas"
  },
  "authors": [
    {
      "name": "YangHaiquan",
      "email": "iwannamaybe@vip.qq.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "5.5.*",
    "illuminate/http": "5.5.*",
    "illuminate/routing":"5.5.*",
    "symfony/translation": "~2.6 || ~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.5",
    "illuminate/config": "^5.0,<5.6"
  },
  "autoload": {
    "psr-4": {
      "Iwannamaybe\\PhpCas\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "master-dev"
    },
    "laravel": {
      "providers": [
        "Iwannamaybe\\PhpCas\\PhpCasServiceProvider"
      ],
      "aliases": {
        "PhpCas": "Iwannamaybe\\PhpCas\\Facades\\PhpCas"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
